{
  "name": "case-booking-app",
  "version": "1.3.4",
  "cacheVersion": "1.0.11",
  "homepage": "/",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.50.5",
    "@tanstack/react-query": "^5.87.4",
    "@testing-library/react": "^13.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "bcryptjs": "^3.0.2",
    "react": "^18.2.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=8192' react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typecheck": "npx tsc --noEmit",
    "lint": "eslint src --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm run typecheck && npm run lint && npm test -- --coverage --watchAll=false",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.6.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^6.1.0",
    "gh-pages": "^6.3.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^1.3.5",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "react-app-rewired": "^2.2.1",
    "supabase": "^2.31.4",
    "supertest": "^7.1.4"
  }
}
